        "# Tổng doanh thu thu được là bao nhiêu và thay đổi như thế nào theo thời gian? Vẽ biểu đồ\n",
        "\n",
        "# Tính tổng doanh thu theo thời gian\n",
        "order['Transaction_value'] = payment['payment_value']\n",
        "order['order_purchase_timestamp'] = pd.to_datetime(order['order_purchase_timestamp'])\n",
        "order['month_year'] = order['order_purchase_timestamp'].dt.to_period('M')\n",
        "total_revenue_by_month = order.groupby('month_year')['Transaction_value'].sum().reset_index()\n",
        "\n",
        "# Vẽ biểu đồ\n",
        "total_revenue_by_month['month_year'] = total_revenue_by_month['month_year'].astype(str)\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.plot(total_revenue_by_month['month_year'], total_revenue_by_month['Transaction_value'])\n",
        "plt.xlabel('Month - Year')\n",
        "plt.ylabel('Total revenue')\n",
        "plt.title('Total revenue over time')\n",
        "plt.xticks(rotation=45)\n",
        "plt.grid(True)\n",
        "plt.show()\n",
        "\n",
        "total_revenue = order['Transaction_value'].sum()\n",
        "print(f'Total revenue: {total_revenue}')\n"
      ],
