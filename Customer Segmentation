        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "\n",
        "# Calculate the medians\n",
        "recency_median = np.median(potential_churned['Recency'])\n",
        "monetary_median = np.median(potential_churned['Monetary'])\n",
        "\n",
        "# Create figure and axes\n",
        "fig, axes = plt.subplots(1, 2, figsize=(8, 6))\n",
        "\n",
        "# Plot Recency\n",
        "axes[0].hist(potential_churned['Recency'], bins=30, color='orange', alpha=0.7)\n",
        "axes[0].axvline(recency_median, color='red', linestyle='--', linewidth=2, label=f'Median: {recency_median:.0f}')\n",
        "axes[0].set_title('Recency of At Risk customers')\n",
        "axes[0].set_xlabel('Recency')\n",
        "axes[0].set_ylabel('Number of customers')\n",
        "axes[0].legend()\n",
        "\n",
        "# Plot Monetary\n",
        "axes[1].hist(potential_churned['Monetary'], bins=30, color='green', alpha=0.7)\n",
        "axes[1].axvline(monetary_median, color='blue', linestyle='--', linewidth=2, label=f'Median: {monetary_median:.0f}')\n",
